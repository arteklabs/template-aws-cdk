{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "app",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "preLaunchTask": "tsc: build - tsconfig.json",
            "program": "${workspaceFolder}/bin/app.ts",
            "outFiles": [
                "${workspaceFolder}/dist/bin/*.js"
            ]
        }
    ]
}